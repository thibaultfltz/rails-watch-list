<div class="container">
  <h1>Lists</h1>
  <% @lists.each do |list| %>
    <ul>
      <li><%= link_to(list.name, list_path(list)) %></li>
    </ul>
  <% end %>

  <p>Create a new list</p>
  <%= simple_form_for(@list) do |f| %>
  <div class="list-name">
    <%= f.input :name %>
  </div>
  <%= f.submit %>
  <% end %>
</div>



